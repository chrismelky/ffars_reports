		<parameter name="facility_id" class="java.lang.Long"/>
		<parameter name="financial_year_id" class="java.lang.Long"/>
		<parameter name="month_date" class="java.util.Date"/>
		<queryString language="SQL">
			<![CDATA[select
    case when pr.funding_source_code in ('80E','10A', '10B', '10E', '80D','80A','30C', '80C') then 'Own Source' else 'Grant' end as fund,
    gc.code as gfs_code,
    gc.name as gfs_description,
    TO_CHAR($P{month_date}::DATE, 'Month') as currentMonth,
     CASE
        WHEN EXTRACT(MONTH FROM ($P{month_date})::DATE) = 7 THEN '-'
         WHEN EXTRACT(MONTH FROM ($P{month_date})::DATE) = 8 THEN 'July'
        ELSE
    'July' || '-' || to_char((DATE_TRUNC('MONTH', ($P{month_date})::DATE) - INTERVAL '0 MONTH + 1 day')::DATE, 'Month')
     END
     as previousMonths ,
         CASE
        WHEN EXTRACT(MONTH FROM ($P{month_date})::DATE) = 7 THEN 'July'
        ELSE
    'July' || '-' || to_char((DATE_TRUNC('MONTH', ($P{month_date})::DATE) - INTERVAL '0 MONTH + 0 day')::DATE, 'Month')
     END
     as allmonths ,
    sum(amount) as budget,
    sum(coalesce(lc.previous2Amount,0)) as previous2Amount,
    sum(coalesce(mc.currentAmount,0)) as currentAmount
from revenue_projections as pr
join gfs_codes gc on pr.gfs_code = gc.code
left join (
    select
        gc.id as gfs_id,
        sum(ge.cr_amount - ge.dr_amount) as currentAmount
    from  gl_entry_groups geg
              join gl_entries ge on geg.id = ge.gl_entry_group_id
              join gl_accounts ga on ge.account = ga.code
              join gfs_codes gc on gc.code = ga.gfs_code
    where ga.gl_account_type =  'REVENUE'
      and geg.apply_date >=(DATE_TRUNC('MONTH', ($P{month_date})::DATE) - INTERVAL '0 MONTH')::DATE
      and geg.apply_date <=(DATE_TRUNC('MONTH', ($P{month_date})::DATE) + INTERVAL '1 MONTH - 1 day')::DATE
      and geg.facility_id = $P{facility_id}
    group by  gc.id
) as mc on mc.gfs_id = gc.id
left join (
    select
        gc.id as gfs_id,
        sum(ge.cr_amount - ge.dr_amount) as previous2Amount
    from  gl_entry_groups geg
              join gl_entries ge on geg.id = ge.gl_entry_group_id
              join gl_accounts ga on ge.account = ga.code
              join gfs_codes gc on gc.code = ga.gfs_code
    where ga.gl_account_type =  'REVENUE'
      and geg.apply_date <= (DATE_TRUNC('MONTH', ($P{month_date})::DATE) - INTERVAL '0 MONTH + 1 day')::DATE
      and geg.apply_date >= (select start_date from financial_years where id = $P{financial_year_id})
      and geg.facility_id = $P{facility_id}
    group by  gc.id
) as lc on lc.gfs_id = gc.id
where pr.financial_year_id= $P{financial_year_id}
  and pr.facility_id = $P{facility_id}
group by pr.funding_source_code, gc.id
order by fund desc;]]>
		</queryString>
