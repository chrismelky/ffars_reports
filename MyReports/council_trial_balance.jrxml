		<parameter name="location_id" class="java.lang.Long"/>
		<parameter name="end_date" class="java.sql.Date"/>
		<queryString language="SQL">
			<![CDATA[SELECT
            row_number() over() as sn,
            gfs_code,
            gfs_description,
            sum(dr_amount) as dr_amount,
            sum(cr_amount) as cr_amount

from (
         SELECT
             tv.gfs_code,
             tv.gfsname as gfs_description,
             case when tv.account_nature = 'DEBIT' then sum(tv.dr_amount - tv.cr_amount) else 0.00 end as dr_amount,
             case when tv.account_nature = 'CREDIT' then sum(tv.cr_amount - tv.dr_amount) else 0.00 end as cr_amount
         from transactions_view tv

         where tv.cid =  $P{location_id}
           and tv.apply_date <=  $P{end_date}
           and (tv.account_type = 'PHYSICAL/REAL' OR tv.apply_date >= (select start_date from financial_years where '2022-12-30' between start_date and end_date) )
         group by tv.gfs_code, tv.gfsname, tv.account_nature
         UNION

         SELECT
             '63293101' as  gfs_code,
             'Accumulated Surplus/Deficit' as gfs_description,
             0.00 as dr_amount,
             sum(tv.cr_amount - tv.dr_amount) as cr_amount
         from transactions_view tv
         where tv.cid =  $P{location_id}
           and (tv.account_type = 'NOMINAL'
           AND tv.apply_date < (select start_date from financial_years where  $P{end_date} between start_date and end_date) )
     )sub
group by gfs_code, gfs_description]]>
		</queryString>
